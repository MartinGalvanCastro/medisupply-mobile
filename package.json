{
  "name": "medisupply-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "detox test",
    "test:e2e:build:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:build:android": "detox build --configuration android.emu.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "generate:api": "orval --config orval.config.ts",
    "generate:component": "node scripts/generate-component.js",
    "generate:hook": "node scripts/generate-hook.js",
    "generate:provider": "node scripts/generate-provider.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "prebuild:ios": "expo prebuild --platform ios",
    "prebuild:android": "expo prebuild --platform android",
    "build:ios": "eas build --profile preview --platform ios",
    "build:android": "eas build --profile preview --platform android",
    "build:all": "eas build --profile preview --platform all"
  },
  "dependencies": {
    "expo": "~52.0.0",
    "expo-router": "~4.0.0",
    "expo-status-bar": "~2.0.0",
    "expo-splash-screen": "~0.29.0",
    "expo-linking": "~7.0.0",
    "expo-constants": "~17.0.0",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "@tanstack/react-query": "^5.59.0",
    "axios": "^1.7.0",
    "zustand": "^5.0.0",
    "@gluestack-ui/themed": "^1.1.58",
    "@gluestack-style/react": "^1.0.58",
    "react-native-svg": "15.8.0",
    "i18next": "^23.15.0",
    "react-i18next": "^15.0.0",
    "expo-localization": "~16.0.0",
    "react-native-mmkv": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~18.3.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.0",
    "typescript": "^5.6.0",
    "detox": "^20.27.0",
    "@config-plugins/detox": "^8.0.0",
    "orval": "^7.3.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "ts-jest": "^29.2.0",
    "@testing-library/react-native": "^12.8.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/jest-native": "^5.4.3",
    "prettier": "^3.4.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11"
  },
  "private": true,
  "packageManager": "yarn@4.5.3"
}
