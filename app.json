{
  "expo": {
    "name": "medisupply-mobile",
    "slug": "medisupply-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "medisupplymobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.medisupplymobile",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take photos of medical supplies and equipment.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select and upload images of medical supplies.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save photos to your photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.medisupplymobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "Camera",
            "PhotoLibrary",
            "PhotoLibraryAddOnly"
          ],
          "androidPermissions": [
            "android.permission.CAMERA",
            "android.permission.READ_EXTERNAL_STORAGE",
            "android.permission.READ_MEDIA_IMAGES",
            "android.permission.READ_MEDIA_VIDEO"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
