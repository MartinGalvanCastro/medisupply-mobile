# Default Environment Configuration
# Copy this to .env.local and customize for your local development

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:8000

# MSW Mock Configuration
# Set to 'true' to enable API mocking for development
EXPO_PUBLIC_ENABLE_MOCKS=true
EXPO_PUBLIC_MOCK_LOGGING=true
EXPO_PUBLIC_MOCK_DELAY=750

# Environment
NODE_ENV=development
